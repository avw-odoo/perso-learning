<openerp>
    <data>
        <template id="listing">
          <t t-call="website.layout">
            <div id="wrap">
            <t t-set="title">Properties listing</t>
              <div class="oe_structure">
                <div class="container">
                  <div class="container">
                    <div class="oe_structure">
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-8">
                      <div class="oe_structure">
                        <div>
                          <ul>
                            <li t-foreach="objects" t-as="object">
                              <a t-attf-href="#{ root }/objects/#{ object.id }">
                                <t t-esc="object.display_name"/>
                              </a>
                            </li>
                          </ul>
                        </div>
                      </div>
                    </div>
                    <div class="col-md-4">
                      <div class="oe_structure">
                        <div>
                          
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            </t>
        </template>
        <template id="object">
          <t t-call="website.layout">
            <div id="wrap">
              <t t-set="title">Properties details</t>
                <div class="row">
                    <div class="container">
                    <div class="oe_structure">
                    <h1><t t-esc="object.display_name"/></h1>
                    <dl>
                      <t t-foreach="object._fields" t-as="field">
                        <dt><p><t t-esc="field"/></p></dt>
                        <dd><p><t t-esc="object[field]"/></p></dd>
                        <p>------</p>
                      </t>
                    </dl>
                  </div>
                  </div>
                </div>
            </div>
          </t>
        </template>
    </data>
</openerp>