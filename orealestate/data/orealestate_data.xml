<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- Realestate property kind data -->
        <record id="property_kind_1" model="orealestate.property">
            <field name="name">Houses</field>
        </record>
        <record id="property_kind_2" model="orealestate.property">
            <field name="name">Offices</field>
        </record>
        <record id="property_kind_3" model="orealestate.property">
            <field name="name">Industries</field>
        </record>
        <record id="property_kind_4" model="orealestate.property">
            <field name="name">Investment property</field>
        </record>
        <record id="property_kind_5" model="orealestate.property">
            <field name="name">Shops</field>
