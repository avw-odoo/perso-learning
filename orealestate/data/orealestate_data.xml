        <record id="property_kind_8" model="orealestate.property">
            <field name="name">Garages</field>
        </record>
        <record id="property_kind_9" model="orealestate.property">
            <field name="name">Other</field>
        </record>
        <record id="property_kind_10" model="orealestate.property">
            <field name="name">Houses promotions</field>
        </record>
        <record id="property_kind_11" model="orealestate.property">
            <field name="name">Apartments promotions</field>
        </record>

        <!-- Realestate property area data -->
        <record id="property_room_1" model="orealestate.area">
            <field name="name">Bedroom</field>
        </record>
        <record id="property_room_2" model="orealestate.area">
            <field name="name">Kitchen</field>
        </record>
        <record id="property_room_3" model="orealestate.area">
            <field name="name">Hall</field>
        </record>
        <record id="property_room_4" model="orealestate.area">
            <field name="name">Office room</field>
        </record>
        <record id="property_room_5" model="orealestate.area">
            <field name="name">Living room</field>
        </record>
        <record id="property_room_6" model="orealestate.area">
            <field name="name">Bathroom</field>
        </record>
        <record id="property_room_7" model="orealestate.area">
            <field name="name">Dining room</field>
        </record>
        <record id="property_room_8" model="orealestate.area">
            <field name="name">Garage</field>
        </record>
        <record id="property_room_9" model="orealestate.area">
            <field name="name">Terrace</field>
        </record>
        <record id="property_room_10" model="orealestate.area">
            <field name="name">Land area</field>
            <field name="specific">True</field>
        </record>
        <record id="property_room_11" model="orealestate.area">
            <field name="name">Other</field>
        </record>
        <record id="property_room_12" model="orealestate.area">
            <field name="name">Living area</field>
            <field name="specific">True</field>
        </record>

        <!-- Realestate property stages data -->
        <record model="crm.stage" id="orealestate_stage_lead1">
            <field name="name">Prospecting</field>
            <field name="probability">10</field>
            <field name="on_change">1</field>
            <field name="sequence">1</field>
            <field name="type">both</field>
        </record>
        <record model="crm.stage" id="orealestate_stage_lead2">
            <field name="name">Negotiation</field>
            <field name="probability">35</field>
            <field name="on_change">1</field>
            <field name="sequence">1</field>
            <field name="type">opportunity</field>
        </record>
        <record model="crm.stage" id="orealestate_stage_lead3">
            <field name="name">Preparation</field>
            <field name="probability">80</field>
            <field name="on_change">1</field>
            <field name="sequence">1</field>
            <field name="type">opportunity</field>
        </record>
        <record model="crm.stage" id="orealestate_stage_lead4">
            <field name="name">Under contract</field>
            <field name="probability">90</field>
            <field name="on_change">1</field>
            <field name="sequence">1</field>
            <field name="type">opportunity</field>
        </record>
        <record model="crm.stage" id="orealestate_stage_lead5">
            <field name="name">Sold</field>
            <field name="fold" eval="True"/>
            <field name="probability">100</field>
            <field name="on_change">1</field>
            <field name="sequence">70</field>
            <field name="type">opportunity</field>
        </record>


