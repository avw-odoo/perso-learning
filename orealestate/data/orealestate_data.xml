        <!-- Realestate mandate sales team -->
        <record id="orealestate_mandate_team" model="crm.team">
            <field name="use_leads">True</field>
            <field name="name">Mandates</field>
            <field name="stage_ids" eval="[(4, ref('orealestate_stage_lead1')),(4, ref('orealestate_stage_lead2')),(4, ref('orealestate_stage_lead3')),(4, ref('orealestate_stage_lead4')),(4, ref('orealestate_stage_lead5'))]"></field>
