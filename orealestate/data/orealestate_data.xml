            <field name="name">House promotion</field>
        </record>
        <record id="property_kind_11" model="orealestate.property">
            <field name="name">Apartment promotion</field>
        </record>

        <!-- Realestate property area data -->
        <record id="property_room_1" model="orealestate.area">
            <field name="name">Bedroom</field>
        </record>
        <record id="property_room_2" model="orealestate.area">
            <field name="name">Kitchen</field>
